<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
   version="2.0">
   <application>
    <resource-bundle>
      <base-name>messages</base-name>
      <var>msgs</var>
    </resource-bundle>
   </application>
  <navigation-rule>
	<from-view-id>/login.xhtml</from-view-id>
	<navigation-case>
	<from-outcome>success</from-outcome>
	<to-view-id>/redirect.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	<from-outcome>loginError</from-outcome>
	<to-view-id>/login.xhtml</to-view-id>
	</navigation-case>
  </navigation-rule>
  <navigation-rule>
   <from-view-id>/redirect.xhtml</from-view-id>
   <navigation-case>
	<from-outcome>start</from-outcome>
	<to-view-id>/workspace.xhtml</to-view-id>
	</navigation-case>
  </navigation-rule> 
  <navigation-rule>
   <from-view-id>/signup.xhtml</from-view-id>
   <navigation-case>
	<from-outcome>success</from-outcome>
	<to-view-id>/redirect.xhtml</to-view-id>
	</navigation-case>
  </navigation-rule>
  <navigation-rule>
   <from-view-id>/start.xhtml</from-view-id>
   <navigation-case>
	<from-outcome>success</from-outcome>
	<to-view-id>/workspace.xhtml</to-view-id>
	</navigation-case>
  </navigation-rule>
  <navigation-rule>
   <from-view-id>/create.xhtml</from-view-id>
   <navigation-case>
	<from-outcome>done</from-outcome>
	<to-view-id>/workspace.xhtml</to-view-id>
	</navigation-case>
  </navigation-rule>
</faces-config>
